{
    "name": "@acm/web",
    "version": "1.0.0",
    "private": true,
    "description": "Anti-Call Masking Platform - Web Dashboard",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint src --ext ts,tsx --fix",
        "type-check": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "codegen": "graphql-codegen --config codegen.ts"
    },
    "dependencies": {
        "@ant-design/icons": "^5.2.6",
        "@ant-design/pro-components": "^2.6.43",
        "@refinedev/antd": "^5.37.0",
        "@refinedev/cli": "^2.16.0",
        "@refinedev/core": "^4.45.0",
        "@refinedev/graphql": "^6.5.0",
        "@refinedev/react-router-v6": "^4.5.0",
        "@tanstack/react-query": "^5.17.0",
        "antd": "^5.13.0",
        "graphql": "^16.8.1",
        "graphql-ws": "^5.14.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.0",
        "recharts": "^2.10.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/typescript": "^4.0.1",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@graphql-codegen/typescript-react-query": "^6.0.0",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/react": "^14.1.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "@vitejs/plugin-react": "^4.2.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "typescript": "^5.3.0",
        "vite": "^5.0.0",
        "vitest": "^1.1.0"
    }
}